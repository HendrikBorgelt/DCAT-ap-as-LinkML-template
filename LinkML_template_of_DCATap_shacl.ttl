# metamodel_version: 1.7.0
@prefix adms: <http://www.w3.org/ns/adms> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sh1: <http://www.w3.org/ns/shacl> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dct:LicenseDocument a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class skos:Concept ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path dct:type ] ;
    sh:targetClass dct:LicenseDocument .

dct:Location a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dct:Location .

dct:PeriodOfTime a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dct:PeriodOfTime .

dcat:Catalog a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class sh1:TemporalLiteral ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path dct:modified ],
        [ sh:class rdfs:Literal ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path dct:description ],
        [ sh:class sh1:TemporalLiteral ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path dcat:themeTaxonomy ],
        [ sh:class dct:LinguisticSystem ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path dct:language ],
        [ sh:class foaf:Document ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path foaf:homepage ],
        [ sh:class rdfs:Literal ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path dct:title ],
        [ sh:class dct:RightsStatement ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path dct:rights ],
        [ sh:class skos:ConceptScheme ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path dcat:themeTaxonomy ] ;
    sh:targetClass dcat:Catalog .

dcat:CatalogRecord a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dcat:CatalogRecord .

dcat:DataService a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class skos:Concept ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path adms:status ],
        [ sh:class skos:Concept ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path adms:status ],
        [ sh:class dct:LinguisticSystem ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path dct:language ],
        [ sh:class sh1:TemporalLiteral ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path dcat:themeTaxonomy ],
        [ sh:class rdfs:Literal ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path dct:title ],
        [ sh:class rdfs:Literal ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path dct:description ],
        [ sh:class dct:Standrad ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path dct:conformsTo ] ;
    sh:targetClass dcat:DataService .

dcat:Dataset a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dcat:Dataset .

dcat:DatasetSeries a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dcat:DatasetSeries .

dcat:Distribution a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dcat:Distribution .

foaf:Agent a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path foaf:name ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path skos:Concept ] ;
    sh:targetClass foaf:Agent .

dct:RightsStatement a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dct:RightsStatement .

dct:Standrad a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dct:Standrad .

skos:ConceptScheme a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass skos:ConceptScheme .

foaf:Document a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass foaf:Document .

dct:LinguisticSystem a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass dct:LinguisticSystem .

sh1:TemporalLiteral a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass sh1:TemporalLiteral .

rdfs:Literal a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ),
        ( rdf:type ) ;
    sh:targetClass rdfs:Literal .

skos:Concept a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass skos:Concept .


